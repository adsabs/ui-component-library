{
  "name": "ui-component-library",
  "version": "1.0.0",
  "description": "UI Component Library for Bumblebee",
  "author": "thostetler",
  "license": "MIT",
  "repository": "adsabs/ui-component-library",
  "main": "dist/index.js",
  "main.umd": "dist/index.umd.production.min.js",
  "module": "dist/index.esm.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build --env production --format umd --name index",
    "start": "tsdx watch --name index --format umd,esm",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@fluentui/react": "^7.106.0",
    "@types/react-google-recaptcha": "^2.1.2",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-scripts": "^3.4.1",
    "tslint": "^6.1.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@hookform/resolvers": "^0.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/diff": "^4.0.2",
    "@types/react": "^16.9.27",
    "@types/react-redux": "^7.1.15",
    "@types/react-table": "^7.0.17",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.0.1",
    "@types/underscore": "^1.10.11",
    "@types/yup": "^0.26.36",
    "diff": "^4.0.2",
    "eslint-plugin-react-hooks": "^4.0.2",
    "formik": "^2.1.4",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-async": "^10.0.1",
    "react-bootstrap": "^1.0.1",
    "react-data-table-component": "^6.9.1",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^16.13.1",
    "react-flexview": "^5.0.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^6.5.3",
    "react-popup": "^0.10.0",
    "react-redux": "^7.2.0",
    "react-table": "^7.2.0",
    "react-window": "^1.8.5",
    "rfc6902": "^5.0.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "styled-components": "^5.1.0",
    "tsdx": "^0.13.2",
    "underscore": "^1.10.2",
    "yup": "^0.28.3"
  },
